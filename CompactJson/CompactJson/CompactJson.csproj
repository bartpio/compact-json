<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Authors>Bart Piotrowski</Authors>
    <Description>Provides compact serialization and deserialization of IEnumerable&lt;IDictionary&lt;string, object&gt;&gt; record sets</Description>
    <Copyright>(c) 2021 Contributors. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/bartpio/compact-json</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bartpio/compact-json</RepositoryUrl>
    <PackageId>CompactJsonCodec</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.0" />
  </ItemGroup>

</Project>
